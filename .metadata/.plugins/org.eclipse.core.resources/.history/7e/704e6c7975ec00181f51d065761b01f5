package br.com.easypark.backend.data;


import org.springframework.data.jpa.repository.JpaRepository;
import org.springframework.data.jpa.repository.Query;

import br.com.easypark.backend.model.Mesa;
import br.com.easypark.backend.model.Produto;

public interface MesaDAO extends JpaRepository<Mesa,Long>{

	@Query("SELECT c FROM Mesa m where m.qrCode == ?1")
	Mesa findByQrCode(String qrCode);
}
